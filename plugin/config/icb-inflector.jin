;
; The locale to use for inflection (en, fr, ...).  This can also be overloaded in the environment
; variable `LOCALE`.  It is recommended you use the environment variable over this value.
;

locale = en

[application]

	aliases = {
		"ICanBoogie\\Inflector": "Inflector"
	}

	delegates = [
		"Hiraeth\\Inflector\\InflectorDelegate"
	]

[twig]

	globals = {
		"inflector": "ICanBoogie\\Inflector"
	}
